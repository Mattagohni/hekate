#!/usr/bin/env php
<?php

require __DIR__.'/../vendor/autoload.php';

use sd\hekate\commands\BitBucketRepoListCommand;
use sd\hekate\services\BitbucketAuthentication;
use Symfony\Component\Console\Application;

$application = new Application();

/**
 * 1. bitbucket:repo-list
 */
$bitBucketListCommand = new BitBucketRepoListCommand();
$bitBucketAuthentication = new BitbucketAuthentication();
$bitBucketListCommand->setAuthentication($bitBucketAuthentication);

$application->add(new BitBucketRepoListCommand());


$application->run();